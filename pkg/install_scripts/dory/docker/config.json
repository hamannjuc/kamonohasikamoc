  "auths": {
    "{{ $.imageRepo.internal.domainName }}": {
      "auth": {{ printf "admin:%s" $.imageRepo.internal.password | b64enc | quote }}
    },
    "{{ $.hostIP }}:{{ $.dory.artifactRepo.internal.portHub }}": {
      "auth":  {{ "public-user:public-user" | b64enc | quote }}
    },
    "{{ $.hostIP }}:{{ $.dory.artifactRepo.internal.portGcr }}": {
      "auth": {{ "public-user:public-user" | b64enc | quote }}
    },
    "{{ $.hostIP }}:{{ $.dory.artifactRepo.internal.portQuay }}": {
      "auth": {{ "public-user:public-user" | b64enc | quote }}
    }
  }
}
