{
  "auths": {
    "{{ $.harbor.domainName }}": {
      "auth": {{ printf "admin:%s" $.harbor.password | b64enc | quote }}
    },
    "{{ $.hostIP }}:{{ $.dory.nexus.portHub }}": {
      "auth":  {{ "public-user:public-user" | b64enc | quote }}
    },
    "{{ $.hostIP }}:{{ $.dory.nexus.portGcr }}": {
      "auth": {{ "public-user:public-user" | b64enc | quote }}
    },
    "{{ $.hostIP }}:{{ $.dory.nexus.portQuay }}": {
      "auth": {{ "public-user:public-user" | b64enc | quote }}
    }
  }
}
